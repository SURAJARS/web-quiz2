  con1 = parseInt(document.querySelector('input[name = "con1"]:checked').value);
  ifstate1 = parseInt(document.querySelector('input[name = "ifstate1"]:checked').value);
  con2 = parseInt(document.querySelector('input[name = "con2"]:checked').value);
   foam = parseInt(document.querySelector('input[name = "ifstate1"]:checked').j); 
  bed = parseInt(document.querySelector('input[name = "con2"]:checked').k);
  
      
	   
	   
	   result = variable + sub + con + ifstate + com + com1 + sub1 + con1 + ifstate1 + con2;
  
	   count=foam+bed;
	document.getElementById("grade").innerHTML = result;
    //document.getElementById("mark").innerHTML = count;
 grading = [
	   {score:0,feedback:"<b>I don't think you studied</b> correct = 0"},
